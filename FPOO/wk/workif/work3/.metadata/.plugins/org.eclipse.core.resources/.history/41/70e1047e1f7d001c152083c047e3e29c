package modelo;


public class Matematica {
	
	public static void main(String[] args) {
	int val1 = 13;
	int val2 = 3;
	System.out.printf("%d + %d = %d\n",val1,val2,val1+val2);
	System.out.printf("%d - %d = %d\n",val1,val2,val1-val2);
	System.out.printf("%d x %d = %d\n",val1,val2,val1*val2);
	System.out.printf("%d / %d = %d\n",val1,val2,val1/val2);
	System.out.printf("O resto é %d\n",val1%val2);
	
	}
}